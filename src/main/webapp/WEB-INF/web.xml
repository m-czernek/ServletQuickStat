        <servlet-class>cz.endhalf.Servlet</servlet-class>
        <!-- uncomment to override the default value for ProductName-->
        <!--init-param>
            <param-name>ProductName</param-name>
            <param-value>AppName from web.xml init-param; can be used for databases, etc.</param-value>
        </init-param-->
    </servlet>


